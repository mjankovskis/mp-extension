{
  "manifest_version": 2,
  "name": "MotoPower.lv new posts extension",
  "version": "0.10",
  "author": "waplet",
  "permissions": [
    "storage"
  ],
  "icons": {
    "80": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://motopower.lv/",
        "*://www.motopower.lv/",
        "*://motopower.lv/index.php",
        "*://www.motopower.lv/index.php"
      ],
      "js": [
        "vendor/moment.min.js",
        "vendor/moment-timezone.min.js",
        "functions.js",
        "content.js"
      ]
    }
  ]
}
